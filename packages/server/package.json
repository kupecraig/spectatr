{
  "name": "@spectatr/server",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:generate": "prisma generate --env-file env/.env",
    "db:push": "prisma db push --env-file env/.env",
    "db:migrate": "prisma migrate dev --env-file env/.env",
    "db:migrate:deploy": "prisma migrate deploy --env-file env/.env",
    "db:seed": "tsx src/scripts/seed.ts",
    "db:studio": "prisma studio --env-file env/.env",
    "db:reset": "prisma migrate reset --env-file env/.env",
    "lint": "eslint ."
  },
  "dependencies": {
    "@clerk/backend": "^2.31.0",
    "@clerk/express": "^1.7.70",
    "@prisma/client": "^6.5.0",
    "@trpc/server": "^11.0.0",
    "bullmq": "^5.67.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "ioredis": "^5.5.1",
    "rate-limit-redis": "^4.2.0",
    "redis": "^5.10.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.1",
    "@types/node": "^22.14.2",
    "eslint": "^9.17.0",
    "prisma": "^6.5.0",
    "tsx": "^4.20.1",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
